{
	"author": "Paul Williams",
	"name": "p44",
	"description": "Svelte package exposing SVG icons crafted using P45.",
	"type": "module",
	"license": "MIT",
	"version": "0.2.0",
	"keywords": [
		"svg",
		"icons",
		"svelte",
		"plunder"
	],
	"exports": {
		"./package.json": "./package.json",
		"./keywords.js": "./keywords.js",
		"./keywords": "./keywords.js",
		".": {
			"svelte": "./index.js"
		}
	},
	"files": [
		"./index.js",
		"./keywords.js",
		"icons"
	],
	"engines": {
		"node": ">=18",
		"npm": ">=9.8"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/PaulioRandall/p44.git"
	},
	"scripts": {
		"exports": "node ./scripts/generate-exports.js",
		"fmt": "prettier --plugin prettier-plugin-svelte --log-level warn --write .",
		"commit": "npm run fmt && npm run exports && cat ./scripts/ready-to-ship.txt"
	},
	"devDependencies": {
		"p45": "0.7.0",
		"prettier": "3.1.1",
		"prettier-plugin-svelte": "3.1.2"
	},
	"peerDependencies": {
		"svelte": "4.x"
	}
}
